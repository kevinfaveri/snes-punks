{
  "name": "snes-punks",
  "description": "SNES inspired 16-bits version of the normal 8-bit Crypto Punks.",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "dev": "next dev",
    "build": "npx hardhat compile && next build",
    "start": "next start",
    "eth-node": "npx hardhat node",
    "eth-compile": "npx hardhat compile",
    "eth-deploy-local": "npx hardhat run scripts/deploy.js --network localhost",
    "eth-deploy-testnet": "npx hardhat run scripts/deploy.js --network rinkeby",
    "eth-deploy-mainnet": "npx hardhat run scripts/deploy.js --network mainnet",
    "eth-verify-mainnet": "npx hardhat verify --network mainnet 0x81aF96d134549293Fa7475BDbd7C34618d588a88 'SNES Punks' 'SNESP'"
  },
  "dependencies": {
